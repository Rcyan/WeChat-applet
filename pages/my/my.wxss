.my-page{
  color: #939393;
}
.header{
  display: flex;
  flex-direction: column;
  padding: 50rpx 0;
  align-items: center;
  background: #bd2d30;
  color: #939393; 
}
.header .header-img{
  width: 140rpx;
  height: 140rpx;
  border-radius:70rpx; 
  align-items: center;
}
.header text{
  margin-top: 10px;
}
/*我的订单*/
.my-list{
  height: 80rpx;
  border-bottom: 1px solid #dadada;
  background: #f5f5f5;
  padding: 4px;
  line-height: 80rpx;
  position: relative;
  font-size: 14px;
}
.my-list-icon{
  display: inline-block;
  margin-right: 16rpx;
}
.my-list-img{
  width:66rpx; 
  height:66rpx;
  vertical-align: -18rpx;
}
.my-list-text{
  display: inline-block;
}
/*交易*/
.my-deal{
   height: 120rpx;
   background: #f5f5f5;
   border-bottom: 1px solid #dadada;
   padding:6px 0;
}
.my-deal-item{
  display: inline-block;
  width: 20%; 
  font-size: 12px;
  text-align: center;
}
.deal-item-icon{
   /* margin-right: 10px; */
}
.deal-item-icon image{
  width: 66rpx;
  height: 66rpx;
}

/*跳转图标*/
.my-list-jump{
  position: absolute;
  top: 15px;
  right: 15px;
  width: 15px;
  height: 15px;
  background-image:  url(../../images/jump.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
}